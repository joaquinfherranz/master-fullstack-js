//Declaracion de funciones
function main() {
  return 'Hello, World!';
}

function ex1() {
  var nombre = 'joaquin';
  console.assert(nombre=='joaquin', 'NO');  
}

function ex2() {
  console.time("ini");
  console.log("joaquin");
  console.info("joaquin");
  console.warn("joaquin");
  console.timeEnd("ini");
}

function ex3() {
  console.time("ini");
  console.log("%c joaquin", 'background: #ddd; color: #fff');
  console.info("%c joaquin", 'background: #aaa; color: #fff');
  console.warn("%c joaquin", 'background: #555; color: #fff');
  console.timeEnd("ini");
}

function ex4() {
  console.time("ini");
  console.groupCollapsed("grup");
  console.log("joaquin");
  console.info("joaquin");
  console.warn("joaquin");
  console.groupEnd();
  console.timeEnd("ini");  
}

function ex5() {
 //5 - Crea una tabla usando la consola para mostrar el nombre de tus compa√±eros y
 // el puesto que ocupa en la clase.
 var compis = [
   {"nombre":"Juan", "puesto": "pos1"},
   {"nombre":"Joaquin", "puesto": "pos2"}   
   ];
  console.table(compis);
}

//Ejecucion de funciones
ex1();
ex2();
ex3();
ex4();
ex5();

main();